<html>

<head>
  <meta charset="UTF-8">
  <title>Lego Identification Project</title>
 
  <style></style>
</head>

<body>
    <nav>
        <ul>
            <li><a href='./index.html'>Sign Out</a></li>
            <li><a href='./classify/index.html'>Classify</a></li>
            <li><a id='collectionNav' href=''>Collection</a></li>
        </ul>
    </nav>
    <main>
        <h2>My Lego Collection</h2>
        <div id = 'collection'>
          <% lego.forEach(lego => { %>
          <div>
            <img class = "lego-pic" src="<%= lego.imgUrl %>">
            <h3><%= lego.name %></h3>

            <p>Part Number: <%=lego.partNum %></p>

            <p>Quantity: <%=lego.quantity %></p>

          </div>

         <% }) %>
         
        </div>
    </main>


    <footer>

    </footer>


    </body>
    </html>