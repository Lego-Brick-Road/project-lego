<html>

<head>
  <meta charset="UTF-8">
  <title>Lego Identification Project</title>
 
  <style></style>
</head>

<body>
    <header>
        <a href="../public/classify/index.html">classify</a>
    </header>

    <main>
        <h2>My Lego Collection</h2>
        <div id = 'collection'>
          <% lego.forEach(lego => { %>
          <div>
            <img class = "lego-pic" src="<%= lego.imgUrl %>">
            <h3><%= lego.name %></h3>

            <p>Part Number: <%=lego.partNum %></p>

            <p>Quantity: <%=lego.quantity %></p>
          </div>

         <% }) %>
         
        </div>
    </main>


    <footer>

    </footer>


    </body>
    </html>